; Multiple_Shooting IPM settings
ipm
{
  nThreads                              3
  dt                                    0.01
  ipmIteration                          5
  deltaTol                              1e-4
  g_max                                 10.0
  g_min                                 1e-6
  computeLagrangeMultipliers            true
  printSolverStatistics                 true
  printSolverStatus                     false
  printLinesearch                       false
  useFeedbackPolicy                     true
  integratorType                        RK2
  threadPriority                        95

  initialBarrierParameter               1e-4
  targetBarrierParameter                1e-4
  barrierLinearDecreaseFactor           0.2
  barrierSuperlinearDecreasePower       1.5
  barrierReductionCostTol               1e-3
  barrierReductionConstraintTol         1e-3

  fractionToBoundaryMargin              0.995
  usePrimalStepSizeForDual              false

  initialSlackLowerBound                1e-4
  initialDualLowerBound                 1e-4
  initialSlackMarginRate                1e-2
  initialDualMarginRate                 1e-2
}

; Rollout settings
rollout
{
  timeStep                      1e-2
  maxNumStepsPerSecond          100000
  checkNumericalStability       false
}

; MPC settings
mpc
{
  timeHorizon                    2.0   ; [s]
  solutionTimeWindow             0.2   ; [s]
  coldStart                      false

  debugPrint                     false

  mpcDesiredFrequency            250   ; [Hz]
  mrtDesiredFrequency            500   ; [Hz]
}

; legged_balance interface settings
diablo_balance_interface
{
  recompileLibraries       1    ; put this flag to 1 to force recompilation of dynamics library
}

; initial state
initialState
{
  (0,0) 0.0  ; px
  (1,0) 0.0  ; theta
  (2,0) 0.0  ; alpha
  (3,0) 0.0  ; psi
  (4,0) 0.0  ; px_dot
  (5,0) 0.0  ; theta_dot
  (6,0) 0.0  ; alpha_dot
  (7,0) 0.0  ; psi_dot
}

; state weight matrix
Q
{
  scaling 1e+1

  (0,0) 1000.0      ; px
  (1,1) 40.0   ; theta_l
  (2,2) 40.0   ; theta_r
  (3,3) 5000.0   ; theta
  (4,4) 100000.0    ; psi
  (5,5) 250.0    ; px_dot
  (6,6) 1.0   ; theta_l_dot
  (7,7) 1.0   ; theta_r_dot
  (8,8) 1.0     ; theta_dot
  (9,9) 25.0      ; psi_dot
}

; control weight matrix
R
{
  scaling 1e-0

  (0,0)  200.0  ; torque
  (1,1)  200.0  ; torque
  (2,2)  200.0  ; torque
  (3,3)  200.0  ; torque
}

